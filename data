first_std_deviation_start, first_std_deviation_end = mean-std_deviation, mean+std_deviation
second_std_deviation_start, second_std_deviation_end = mean-(2*std_deviation),mean+(2*std_deviation)
third_std_deviation_start, third_std_deviation_end = mean-(3*std_deviation),mean+(3*std_deviation)

thin_1_std_devaiation = [result for result in data if result > first_std_deviation_start and result < first_std_deviation_end]
thin_2_std_devaiation = [result for result in data if result > second_std_deviation_start and result < first_std_deviation_end]
